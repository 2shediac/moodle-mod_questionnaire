{{!
    @template mod_questionnaire/complete

    Template which defines a check type question survey display.

    Classes required for JS:
    * /mod/questionnaire/module.js

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * respondentlink - array: HTML list of all respondent links.

    Example context (json):
    {
    },
    }}
<!-- Begin HTML generated from question_check template. -->
{{#choices}}
{{#checkbox}}
<input id="{{checkbox.id}}" value="{{checkbox.value}}" name="{{checkbox.name}}" type="checkbox" {{#checkbox.checked}}checked="checked"{{/checkbox.checked}} />
<label for="{{checkbox.id}}">{{{checkbox.label}}}</label>
<br />
{{/checkbox}}
{{#other}}
<input id="{{other.id}}" value="{{other.value}}" name="{{other.name}}" type="checkbox" {{#other.checked}}checked="checked"{{/other.checked}} />
<label for="{{other.id}}">{{{other.label}}}</label>
<input size="25" name="{{other.oname}}" onclick="other_check(name)" value="{{other.ovalue}}" type="text" />
<br />
{{/other}}
{{/choices}}
<!-- End HTML generated from question_check template. -->