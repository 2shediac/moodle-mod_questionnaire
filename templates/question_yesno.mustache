{{!
    @template mod_questionnaire/complete

    Template which defines a yes/no type question survey display.

    Classes required for JS:
    * /mod/questionnaire/module.js

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * respondentlink - array: HTML list of all respondent links.

    Example context (json):
    {
    },
    }}
<!-- Begin HTML generated from question_yesno template. -->
{{#choices}}
{{#yes}}
<input id="{{yes.id}}" value="{{yes.value}}" name="{{yes.name}}" type="radio" {{#yes.checked}}checked="checked"{{/yes.checked}} {{#yes.disabled}}disabled="disabled"{{/yes.disabled}} {{#yes.onclick}}onclick="{{yes.onclick}}"{{/yes.onclick}}/>
<label for="{{yes.id}}">{{{yes.label}}}</label>
{{/yes}}
{{#no}}
<input id="{{no.id}}" value="{{no.value}}" name="{{no.name}}" type="radio" {{#no.checked}}checked="checked"{{/no.checked}} {{#no.disabled}}disabled="disabled"{{/no.disabled}} {{#no.onclick}}onclick="{{no.onclick}}"{{/no.onclick}}/>
<label for="{{no.id}}">{{{no.label}}}</label>
{{/no}}
{{#noanswer}}
<input id="{{noanswer.id}}" value="{{noanswer.value}}" name="{{noanswer.name}}" type="radio" {{#noanswer.checked}}checked="checked"{{/noanswer.checked}} {{#noanswer.disabled}}disabled="disabled"{{/noanswer.disabled}} {{#noanswer.onclick}}onclick="{{noanswer.onclick}}"{{/noanswer.onclick}}/>
<label for="{{noanswer.id}}">{{{noanswer.label}}}</label>
{{/noanswer}}
{{/choices}}
<!-- End HTML generated from question_yesno template. -->